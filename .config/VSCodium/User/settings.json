{
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.synctex.gz",
    "_minted-*",
    "*.pygtex",
    "*.pygstyle",
    "*.listing"
  ],
  "latex-workshop.latex.clean.subfolder.enabled": true,
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-shell-escape",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "%DOC%"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-shell-escape",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "%DOCFILE%"
      ]
    }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontLigatures": true,
  "terminal.integrated.rendererType": "dom",
  "workbench.statusBar.feedback.visible": false,
  "latex-workshop.chktex.enabled": true,
  "window.menuBarVisibility": "toggle",
  "workbench.tips.enabled": false,
  "telemetry.enableCrashReporter": false,
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "editor.fontFamily": "'Fira Code Retina','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontSize": 15,
  "editor.rulers": [80],
  "editor.minimap.renderCharacters": false,
  "workbench.colorTheme": "Min Light",
  "cSpell.language": "en,fr-FR",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "go",
    "handlebars",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pub",
    "python",
    "restructuredtext",
    "rust",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  
  "[c]": {
    "editor.rulers": [
      80
    ]
  },
  "[arm]": {
    "editor.rulers": [
      80
    ]
  },
  "C_Cpp.clang_format_style": "{ColumnLimit: 80, IndentWidth: 4, BreakBeforeBraces: Linux}",
  "C_Cpp.updateChannel": "Insiders",
"editor.detectIndentation": false,
"telemetry.enableTelemetry": false,
"gitlens.advanced.telemetry.enabled": false
  
}
